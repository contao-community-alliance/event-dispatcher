{
    "name": "contao-community-alliance/event-dispatcher",
    "description": "Event dispatcher service for Contao Open Source CMS",
    "keywords": [
        "contao",
        "event"
    ],
    "type": "contao-module",
    "homepage": "http://c-c-a.org/",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        },
        {
            "name": "Stefan Heimes",
            "email": "stefan_heimes@hotmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@c-c-a.org",
        "issues": "https://github.com/contao-community-alliance/event-dispatcher/issues",
        "wiki": "https://github.com/contao-community-alliance/event-dispatcher/wiki",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/contao-community-alliance/event-dispatcher"
    },
    "require": {
        "php": ">=5.4",
        "contao/core-bundle": "~3.5 | ~4.0",
        "contao-community-alliance/composer-plugin": "~2.0 | ~3.0",
        "contao-community-alliance/dependency-container": "dev-feature/contao4-proposal2 as 1.9.0",
        "symfony/event-dispatcher": "~2.3 | ~3.0",
        "symfony/http-kernel": "~2.8 | ~3.0"
    },
    "require-dev": {
        "phpcq/all-tasks": "~1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "bit3/contao-event-dispatcher": "self.version"
    },
    "autoload": {
        "psr-4": {
            "ContaoCommunityAlliance\\Contao\\EventDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContaoCommunityAlliance\\Contao\\EventDispatcher\\Test\\": "tests/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "src/Resources/contao": "system/modules/event-dispatcher"
            }
        },
        "branch-alias": {
            "dev-master": "1.3.x-dev",
            "dev-develop": "1.4.x-dev"
        }
    }
}
